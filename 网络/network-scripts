# 配置在/etc/sysconfig/network-scripts，适用于centos7
HWADDR=08:F1:EA:F2:0C:0D
TYPE=Ethernet
PROXY_METHOD=none
BROWSER_ONLY=no
BOOTPROTO=none
IPADDR=192.168.1.183
PREFIX=24
GATEWAY=192.168.1.1
DNS1=192.168.1.1
DNS2=114.114.114.114
DEFROUTE=yes
IPV4_FAILURE_FATAL=no
IPV4_DNS_PRIORITY=100
IPV6INIT=no
NAME=eno2
UUID=985a0a37-d5f4-437e-88e7-1a15ca9ba780
DEVICE=eno2
ONBOOT=yes
PEERDNS=no
